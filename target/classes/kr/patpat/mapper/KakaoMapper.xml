<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.patpat.mapper.KakaoMapper">
	
   <insert id="kakaoJoin" parameterType="java.util.HashMap">
      insert into member_test(memEmail, memNickName) 
      values(#{email}, #{nickname})
   </insert>
   
   <select id="selectKakao" parameterType="java.util.HashMap" resultType="kr.patpat.entity.Kakao">
		select * from member_test where memEmail=#{email} and memNickName=#{nickname}
   </select>

</mapper>